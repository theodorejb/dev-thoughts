{
    "name": "theodorejb/dev-thoughts",
    "description": "Feature poignant quotes and funny sayings related to programming",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Theodore Brown",
            "email": "theodorejb@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "theodorejb\\DevThoughts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "theodorejb\\DevThoughts\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "psalm/plugin-phpunit": "^0.19.5",
        "vimeo/psalm": "^6.14"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "analyze": "psalm",
        "test": "phpunit tests"
    }
}
